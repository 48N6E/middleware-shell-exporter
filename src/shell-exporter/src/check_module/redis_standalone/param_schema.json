{
  "host": {
    "type": "string",
    "regex":"(^((2((5[0-5])|([0-4]\\d)))|([0-1]?\\d{1,2}))(\\.((2((5[0-5])|([0-4]\\d)))|([0-1]?\\d{1,2}))){3}$)|(^svc.*)"
  },
  "port": {
    "type": "int",
    "regex":"[0-9]{1,5}"
  },
  "passwd": {
    "type": "string",
    "regex":"[0-9a-zA-Z!@#$_&*+-;]{0,40}"
  },
  "key": {
    "type": "string",
    "regex":"[0-9a-zA-Z_]{0,40}"
  }
}

